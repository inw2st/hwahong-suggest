# PostgreSQL (Oracle DB)
DATABASE_URL=postgresql://user:password@localhost:5432/suggestions

# Security
JWT_SECRET_KEY=change-me-to-a-long-random-string
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=720

# CORS - 실제 도메인 또는 서버 IP로 변경
CORS_ORIGINS=http://your-server-ip

# VAPID Keys (Push 알림용)
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# 앱 시작 시 테이블 자동 생성
AUTO_CREATE_TABLES=true
